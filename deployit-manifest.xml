<?xml version="1.0" encoding="UTF-8"?>
<udm.DeploymentPackage version="$BUILD_NUMBER" application="BacktraceApp">
	<application />
	<deployables>
		<docker.ContainerSpec name="/backtraceapp-web">
			<containerName>backtraceapp-web</containerName>
			<image>vlussenburg/backtrace-webapp:$BUILD_NUMBER</image>
			<environment>
				<entry key="BACKTRACE_ENDPOINT">{{BACKTRACE_ENDPOINT}}</entry>
				<entry key="BACKTRACE_TOKEN">{{BACKTRACE_TOKEN}}</entry>
				<entry key="APPLICATION_VERSION">$BUILD_NUMBER</entry>
				<entry key="APPLICATION_NAME">backtrace-app</entry>
			</environment>
			<showLogsAfter>5</showLogsAfter>
			<portBindings>
				<docker.PortSpec name="/docker-backtraceapp-web/pm-8080">
					<hostPort>8080</hostPort>
					<containerPort>5000</containerPort>
				</docker.PortSpec>
			</portBindings>
		</docker.ContainerSpec>
		<docker.NetworkSpec name="/docker-backtraceapp-network">
			<tags />
			<networkName>backtraceapp</networkName>
			<driver>bridge</driver>
			<networkOptions />
		</docker.NetworkSpec>
	</deployables>
	<dependencyResolution>LATEST</dependencyResolution>
	<undeployDependencies>false</undeployDependencies>
</udm.DeploymentPackage>